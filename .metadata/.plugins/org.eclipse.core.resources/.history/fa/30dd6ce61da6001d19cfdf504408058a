#include "cpp_main.h"
#include "main.h"		//For. HAL-Functions
#include "stm32f7xx_hal_gpio.h"

void cpp_main(){
	// Output to console
	int a = 1;
	std::cout<<"a = "<< a << (char) ASCII_LF<<(char) ASCII_CR;
	std::cout<<"CPP-Main started! "<< (char) ASCII_LF<<(char) ASCII_CR;
	fflush(stdout);

	while (1)
		{
			HAL_Delay(100);
		}
}


