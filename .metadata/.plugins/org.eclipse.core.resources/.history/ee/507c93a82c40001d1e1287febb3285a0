#include "../../Core_CPP/Inc/NoneBlockSystemTickDelay.h"

NoneBlockSystemTickDelay::NoneBlockSystemTickDelay(float_t aFrequency, uint8_t aDutyCycle)
{
	DelayNoneBlocking_wait = (1000/aFrequency)*(aDutyCycle/100); //in ms
}

NoneBlockSystemTickDelay::~NoneBlockSystemTickDelay() {

}

