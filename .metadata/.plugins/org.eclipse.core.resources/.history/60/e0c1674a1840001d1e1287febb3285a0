#include "../../Core_CPP/Inc/NoneBlockSystemTickDelay.h"

NoneBlockSystemTickDelay::NoneBlockSystemTickDelay(
		uint32_t aDelayNoneBlocking_wait,
		uint32_t aDelayNoneBlocking_tickstart)	:
		DelayNoneBlocking_wait(aDelayNoneBlocking_wait), DelayNoneBlocking_tickstart(aDelayNoneBlocking_tickstart)
{
}

NoneBlockSystemTickDelay::NoneBlockSystemTickDelay(
		float_t aFrequency,
		uint8_t aDutyCycle,
		uint32_t aDelayNoneBlocking_wait,
		uint32_t aDelayNoneBlocking_tickstart)	:
		BlinkingLed(aFrequency, aDutyCycle),
		DelayNoneBlocking_wait(aDelayNoneBlocking_wait),
		DelayNoneBlocking_tickstart(aDelayNoneBlocking_tickstart) {
}

NoneBlockSystemTickDelay::~NoneBlockSystemTickDelay() {

}

