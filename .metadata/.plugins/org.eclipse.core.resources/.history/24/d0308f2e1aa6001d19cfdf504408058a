#include "stm32f7xx_hal_gpio.h"

void cpp_main(){
	// Output to Console
	int cnt = 0;
	printf("CPP Main started!\n");
	printf("Cnt=%d",cnt); printf("\n");

	while (1)
		{
			ObjAccSensor.updateValues();
			ObjAccSensor.show();
			/*if(HAL_GPIO_ReadPin(USER_Btn_GPIO_Port, USER_Btn_Pin))
				ObjAccSensor.setZero();*/
			HAL_Delay(100);
		}
}


