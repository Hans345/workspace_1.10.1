#include "cpp_main.h"
#include <stdint.h>		//For. printf()


void cpp_main(){
	// Output to Console
	uint32_t cnt = 0;
	printf("CPP Main started!\n");
	printf("Cnt=%d",cnt); printf("\n");

	while (1)
		{
			HAL_GPIO_TogglePin(GPIOB, LD2_Blue);
			HAL_Delay(250);
		}
}


